package com.tvu56;

import java.util.Date;

public class Customer {
	private int id;
	private String name;
	private String sex;
	private Date dob;
	
	Customer() {

	}
	
	Customer(int id, String name, String sex, Date dob) {
		this.id = id;
		this.name = name;
		this.sex = sex;
		this.dob = dob;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public Date getDob() {
		return dob;
	}
	public void setDob(Date dob) {
		this.dob = dob;
	}
	
	
	
}
